<?xml version="1.0"?>
<locatingRules xmlns="http://thaiopensource.com/ns/locating-rules/1.0">
  <transformURI fromPattern="*.xml" toPattern="*.rnc"/>

  <uri pattern="*.xsl" typeId="XSLT"/>
  <uri pattern="*.rng" typeId="RELAX NG"/>
  <uri pattern="*.dbk" typeId="DocBook"/>

  <namespace ns="http://www.w3.org/1999/XSL/Transform" typeId="XSLT"/>
  <namespace ns="http://docbook.org/ns/docbook" typeId="DocBook"/>
  <namespace ns="http://www.w3.org/1998/Math/MathML" typeId="MathML"/>
  <namespace ns="http://www.w3.org/2005/Atom" typeId="Atom"/>
  <namespace ns="https://ldbeth.sdf.org/lsml" typeId="LsML"/>
  <namespace ns="urn:speedata.de:2009/publisher/en" typeId="Layout"/>

  <documentElement localName="rss" typeId="RSS"/>
  <documentElement localName="feed" typeId="Atom"/>
  <documentElement prefix="atom" localName="entry" typeId="Atom"/>

  <documentElement prefix="s" localName="schema" typeId="Schematron"/>

  <documentElement prefix="" localName="set" typeId="DocBook"/>
  <documentElement prefix="" localName="article" typeId="DocBook"/>
  <documentElement prefix="" localName="book" typeId="DocBook"/>
  <documentElement prefix="" localName="chapter" typeId="DocBook"/>
  <documentElement prefix="" localName="part" typeId="DocBook"/>
  <documentElement prefix="" localName="refentry" typeId="DocBook"/>
  <documentElement prefix="" localName="section" typeId="DocBook"/>

  <documentElement prefix="" localName="lsml" typeId="LsML"/>
  <documentElement prefix="" localName="TeXML" typeId="TeX"/>

  <documentElement prefix="" localName="Layout" typeId="Layout"/>

  <typeId id="XSLT" uri="xslt.rnc"/>
  <typeId id="Schematron" uri="schematron.rnc"/>
  <typeId id="DocBook" uri="docbookrb.rnc"/>
  <typeId id="MathML" uri="mathml3.rnc"/>
  <typeId id="RSS" uri="rss.rnc"/>
  <typeId id="Atom" uri="atom.rnc"/>
  <typeId id="LsML" uri="lsml.rnc"/>
  <typeId id="TeX" uri="texml.rnc"/>
  <typeId id="Layout" uri="layoutschema.rnc"/>

</locatingRules>
